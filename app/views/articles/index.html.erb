<h1>All Articles</h1>

<table>
  <head>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Actions</th>
    </tr>
  </head>
  <tbody>
    <% @articles.each do |article| %>
      <tr>
        <td><%= article.title %></td>
        <td><%= article.description %></td>
        <td>Some Actions</td>
      </tr>
    <% end %>
  </tbody>
</table>